{
  "version": 2,
  "builds": [
    {
      "src": "serverless/handler.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/webhook",
      "dest": "serverless/handler.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.11"
  },
  "functions": {
    "serverless/handler.py": {
      "runtime": "python3.11",
      "maxDuration": 30
    }
  }
}
